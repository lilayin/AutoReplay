{

  "manifest_version": 1,
  "name": "AutoReplay",
  "version": "1.0",

  "description": "自动回复",

  "icons": {
    "48": "icons/border-48.jpeg"
  },

  "permissions": [
    "activeTab","sessions","alarms","webRequest","tabs","*://*/mytoolkit/*","*://*/crm.meierbei.com/*","webRequest"
  ],

  "browser_action": {
    "default_icon": "icons/border-48.jpeg",
    "default_title": "自动回复",
    "default_popup": "popup/index.html"
  }

}
